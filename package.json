  "license": "ISC",
  "bugs": {
    "url": "https://github.com/malte/me-portfolio/issues"
  },
  "homepage": "https://github.com/malte/me-portfolio#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-navigation": "^0.3.2",
    "autoprefixer": "^10.3.7",
    "luxon": "^2.0.2",
    "markdown-it": "^12.2.0",
    "markdown-it-anchor": "^8.3.1",
    "markdown-it-link-attributes": "^3.0.0",
    "nunjucks": "^3.2.3",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.1",
    "postcss-nested": "^5.0.6",
    "tailwindcss": "^2.2.16"
  },
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.3.0",
    "@tailwindcss/typography": "^0.4.1",
    "eleventy-plugin-vimeo-embed": "^1.3.2"
  }
}
