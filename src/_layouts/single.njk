<article class="main-article min-h-full mt-8 md:mt-36">
<div class="px-5 ">
  <h1 class="article-title font-bold text-5xl">
    {{ title }}
  </h1>
  <div class="description mt-8 cms-content">
    {{ description | safe }}
  </div>
  <div class="mx-auto content my-8 max-w-screen-lg cms-content">
    {{ content | safe }}
  </div>
</div>
</article>